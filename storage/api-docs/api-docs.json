{
    "openapi": "3.0.0",
    "info": {
        "title": "APIs For Foozo Project",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost/api/"
        }
    ],
    "paths": {
        "/api/home": {
            "get": {
                "summary": "Get list of users",
                "operationId": "index",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    }
                }
            }
        }
    },
    "components": {
        "responses": {
            "200": {
                "description": "Logout"
            },
            "400": {
                "description": "Bad request"
            },
            "404": {
                "description": "Resource Not Found"
            }
        },
        "requestBodies": {
            "login": {
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "email": {
                                    "type": "string",
                                    "example": "admin@example.com"
                                },
                                "password": {
                                    "type": "string",
                                    "example": "123456"
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "bearerAuth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}